{
  "name": "Quiz",
  "version": "1.0.0",
  "description": "Quiz training",
  "author": "Alexey Borisov",
  "scripts": {
    "start:server": "go run server/main.go server/handlers.go",
    "start:client": "webpack-dev-server --progress  --colors --watch --content-base ./public --config ./configs/webpack.dev.config.babel.js",
    "test": "eslint ./client/src && jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "yarn run test",
    "prepush": "yarn run test",
    "prod:build": "NODE_ENV=production webpack -p --progress --config ./configs/webpack.prod.config.babel.js"
  },
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-flow-react-proptypes": "^3.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.1.1",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.34.1",
    "extract-text-webpack-plugin": "2.1.2",
    "flow-bin": "^0.49.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "node-xml2json": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-saga": "^0.15.4"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
